                "_media-type": {
                    "type": "string"
                },
                "permissions": {
                    "type": "object",
                    "properties": {
                        "create": {
                            "type": "object",
                            "properties": {
                                "hasAccess": {
                                    "type": "boolean"
                                },
                                "restrictedContentTypeIds": {
                                    "type": "array",
                                    "items": {}
                                },
                                "restrictedLanguageCodes": {
                                    "type": "array",
                                    "items": {}
                                }
                            },
                            "required": [
                                "hasAccess",
                                "restrictedContentTypeIds",
                                "restrictedLanguageCodes"
                            ]
