                                    "type": "boolean"
                                }
                            },
                            "required": [
                                "hasAccess"
                            ]
                        },
                        "hide": {
                            "type": "object",
                            "properties": {
                                "hasAccess": {
                                    "type": "boolean"
                                }
                            },
                            "required": [
                                "hasAccess"
                            ]
                        }
                    },
                    "required": [
                        "create",
                        "edit",
                        "delete",
                        "hide"
                    ]
                }
            },
            "required": [
                "_media-type",
                "permissions"
            ]
        }
    },
    "required": [
        "ContentTreeNodeExtendedInfo"
    ]
}
